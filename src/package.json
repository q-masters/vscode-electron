{
  "name": "electron-vscode",
  "version": "0.0.0",
  "description": "electron installer in vscode extension",
  "main": "./dist/extension.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "vscode:prepublish": "rimraf ./dist && webpack --config ./bin/webpack.conf.js --mode production",
    "prebuild": "rimraf ./dist",
    "prestart": "rimraf ./dist",
    "build": "webpack --config ./bin/webpack.config.js",
    "start": "webpack --config ./bin/webpack.config.js && electron ./dist/electron-app.js"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:MyExtension.helloElectron"
  ],
  "contributes": {
    "commands": [
      {
        "command": "MyExtension.helloElectron",
        "title": "start electron",
        "category": "MyExtension"
      }
    ]
  },
  "author": "Ralf Hannuschka",
  "license": "ISC",
  "devDependencies": {
    "@types/node-fetch": "2.5.7",
    "@types/vscode": "1.52.0",
    "electron": "11.1.1",
    "eslint": "7.13.0",
    "html-webpack-plugin": "5.0.0-alpha.14",
    "node-fetch": "2.6.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "ts-loader": "8.0.11",
    "tsyringe": "4.4.0",
    "typescript": "4.1.3",
    "webpack": "5.4.0",
    "webpack-cli": "4.2.0"
  },
  "dependencies": {}
}
