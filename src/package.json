    "electron": "11.5.0",
    "eslint": "7.20.0",
    "node-fetch": "2.6.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "ts-loader": "8.0.17",
    "tsyringe": "4.4.0",
