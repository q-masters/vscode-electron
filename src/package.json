{
  "name": "electron-vscode",
  "version": "0.0.0",
  "description": "electron installer in vscode extension",
  "main": "./dist/vscode-electron.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "vscode:prepublish": "rimraf ./dist && webpack --config ./bin/webpack-prod.config.js",
    "prebuild": "rimraf ./dist",
    "prestart": "rimraf ./dist",
    "build": "webpack --config ./bin/webpack.config.js"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/q-masters/vscode-electron"
  },
  "activationEvents": [
    "onCommand:qmasters:electron.install",
    "onCommand:qmasters:electron.run"
  ],
  "publisher": "qmasters",
  "contributes": {
    "commands": [
      {
        "command": "qmasters:electron.install",
        "title": "install electron",
        "category": "QMasters"
      },
      {
        "command": "qmasters:electron.run",
        "title": "run electron app",
        "category": "QMasters"
      }
    ]
  },
  "author": "Ralf Hannuschka",
  "license": "ISC",
  "devDependencies": {
    "@types/node-fetch": "2.5.7",
    "@types/vscode": "1.52.0",
    "electron": "11.1.1",
    "eslint": "7.19.0",
    "node-fetch": "2.6.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "ts-loader": "8.0.16",
    "tsyringe": "4.4.0",
    "typescript": "4.1.3",
    "webpack": "5.21.2",
    "webpack-cli": "4.5.0"
  },
  "dependencies": {}
}
